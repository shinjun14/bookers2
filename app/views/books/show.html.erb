<h1>User info</h1>
<%= form_with model: @book, url:'/books', local:true do |f| %>
 <h1>New Book</h1>
 <h4>Title</h4>
 <%= f.text_field :title %>
 <h4>Opinion</h4>
 <%= f.text_area :body %>
